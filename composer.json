{
    "name": "vdeapps/phpcore-datetime",
    "type": "library",
    "description": "Manage Datetime with opened/closed days",
    "keywords": [
        "vdeapps",
        "library",
        "datetime"
    ],
    "homepage": "https://github.com/vdeapps/phpcore-datetime",
    "license": "MIT",
    "authors": [
        {
            "name": "vdeApps",
            "email": "vdeapps@riseup.net",
            "homepage": "https://github.com/vdeapps/phpcore-datetime",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "vdeapps@riseup.net",
        "issues": "https://github.com/vdeapps/phpcore-datetime/issues"
    },
    "config": {
        "platform": {
            "php": "5.6.99"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "composer/composer": "^1.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "vdeApps\\phpCore\\": "src/",
            "PHPTDD\\": "tests/unit/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix . --fixers=-psr0",
        "phpcs": "vendor/squizlabs/php_codesniffer/bin/phpcs src",
        "phpcbf": "vendor/squizlabs/php_codesniffer/bin/phpcbf src"
    }
}